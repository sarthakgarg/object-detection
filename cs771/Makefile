all:
	mkdir Bicycle
	mkdir Car
	mkdir Motorcycle
	mkdir Number-plate
	mkdir Autorickshaw
	mkdir Rickshaw
	mkdir Person

clean:
	rm -r data/Bicycle
	rm -r data/Car
	rm -r data/Motorcycle
	rm -r data/Number-plate
	rm -r data/Autorickshaw
	rm -r data/Rickshaw
	rm -r data/Person
	rm -r Person
	rm -r Bicycle
	rm -r Car
	rm -r Motorcycle
	rm -r Number-plate
	rm -r Autorickshaw
	rm -r Rickshaw

move:
	mv Bicycle data
	mv Car data
	mv Motorcycle data
	mv Number-plate data
	mv Autorickshaw data
	mv Rickshaw data
	mv Person data
